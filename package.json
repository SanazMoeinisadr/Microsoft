    "xl": "node-sass -w scss/xl.scss -o css/ -r --output-style compressed",
    "lg": "node-sass -w scss/lg.scss -o css/ -r --output-style compressed",
    "md": "node-sass -w scss/md.scss -o css/ -r --output-style compressed",
    "sm": "node-sass -w scss/sm.scss -o css/ -r --output-style compressed",
    "xs": "node-sass -w scss/xs.scss -o css/ -r --output-style compressed"
  },
